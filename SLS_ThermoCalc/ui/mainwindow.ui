/* 全局字体和背景 */
* {
    font-family: "SF Pro Display", "PingFang SC", "Microsoft YaHei", sans-serif;
}

QMainWindow {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                stop:0 #0a0e17, stop:0.5 #0d1321, stop:1 #0a1628);
}

/* ===== 分组框样式 ===== */
QGroupBox {
    font-weight: 600;
    font-size: 13px;
    border: 1px solid rgba(0, 180, 216, 0.3);
    border-radius: 12px;
    margin-top: 16px;
    padding: 16px 12px 12px 12px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 rgba(13, 21, 32, 0.95), stop:1 rgba(10, 18, 28, 0.98));
    color: #e0e6ed;
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 16px;
    top: 2px;
    padding: 2px 12px;
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                stop:0 #00b4d8, stop:1 #0096c7);
    border-radius: 8px;
    color: #ffffff;
    font-weight: bold;
    font-size: 12px;
}

/* ===== 标签样式 ===== */
QLabel {
    color: #a8b8c8;
    font-size: 12px;
    font-weight: 500;
}
QLabel#lblEngineInfo {
    color: #5a7a9a;
    font-style: italic;
    font-size: 11px;
    padding: 4px 0;
}
QLabel#lblTitle {
    color: #00b4d8;
    font-size: 18px;
    font-weight: bold;
}
QLabel#lblSubtitle {
    color: #5a7a9a;
    font-size: 11px;
}

/* ===== 输入控件样式 ===== */
QLineEdit, QDoubleSpinBox, QSpinBox {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #0f1926, stop:1 #0a1320);
    border: 1px solid rgba(30, 58, 95, 0.8);
    border-radius: 8px;
    padding: 8px 12px;
    color: #e0e6ed;
    font-size: 13px;
    font-weight: 500;
    selection-background-color: #00b4d8;
}
QLineEdit:focus, QDoubleSpinBox:focus, QSpinBox:focus {
    border: 2px solid #00b4d8;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #121f2e, stop:1 #0d1825);
}
QLineEdit:read-only {
    background: rgba(30, 58, 95, 0.3);
    color: #7a9ab8;
}
QDoubleSpinBox::up-button, QSpinBox::up-button,
QDoubleSpinBox::down-button, QSpinBox::down-button {
    width: 20px;
    border: none;
    background: transparent;
}
QDoubleSpinBox::up-arrow, QSpinBox::up-arrow {
    image: none;
    border-left: 4px solid transparent;
    border-right: 4px solid transparent;
    border-bottom: 5px solid #00b4d8;
}
QDoubleSpinBox::down-arrow, QSpinBox::down-arrow {
    image: none;
    border-left: 4px solid transparent;
    border-right: 4px solid transparent;
    border-top: 5px solid #00b4d8;
}

/* ===== 下拉框样式 ===== */
QComboBox {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #0f1926, stop:1 #0a1320);
    border: 1px solid rgba(30, 58, 95, 0.8);
    border-radius: 8px;
    padding: 10px 14px;
    color: #e0e6ed;
    font-size: 13px;
    font-weight: 500;
    min-height: 20px;
}
QComboBox:hover {
    border: 1px solid rgba(0, 180, 216, 0.6);
}
QComboBox:focus {
    border: 2px solid #00b4d8;
}
QComboBox::drop-down {
    border: none;
    width: 30px;
    background: transparent;
}
QComboBox::down-arrow {
    image: none;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-top: 7px solid #00b4d8;
}
QComboBox QAbstractItemView {
    background-color: #0d1520;
    border: 1px solid #1e3a5f;
    border-radius: 8px;
    selection-background-color: rgba(0, 180, 216, 0.3);
    color: #e0e6ed;
    padding: 4px;
    outline: none;
}
QComboBox QAbstractItemView::item {
    padding: 8px 12px;
    border-radius: 4px;
    min-height: 24px;
}
QComboBox QAbstractItemView::item:hover {
    background-color: rgba(0, 180, 216, 0.2);
}

/* ===== 按钮样式 ===== */
QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #00c4e8, stop:1 #0096c7);
    color: #ffffff;
    border: none;
    border-radius: 10px;
    padding: 12px 28px;
    font-weight: bold;
    font-size: 14px;
    min-height: 20px;
}
QPushButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #48daf4, stop:1 #00b4d8);
}
QPushButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #0096c7, stop:1 #007ba7);
}
QPushButton:disabled {
    background: rgba(30, 58, 95, 0.5);
    color: #4a5a6a;
}
QPushButton#btnCalculate {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                stop:0 #00b4d8, stop:0.5 #00c4e8, stop:1 #00d4f8);
    font-size: 15px;
    padding: 14px 32px;
    border-radius: 12px;
    min-height: 24px;
}
QPushButton#btnCalculate:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                stop:0 #00c4e8, stop:0.5 #00d4f8, stop:1 #00e4ff);
}
QPushButton#btnClear {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #f06c77, stop:1 #e63946);
}
QPushButton#btnClear:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #ff7c87, stop:1 #f06c77);
}
QPushButton#btnExport {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #40c8b8, stop:1 #2a9d8f);
}
QPushButton#btnExport:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #50d8c8, stop:1 #40c8b8);
}

/* ===== 表格样式 - 清晰易读 ===== */
QTableWidget {
    background-color: rgba(10, 16, 26, 0.95);
    alternate-background-color: rgba(15, 24, 38, 0.95);
    border: 1px solid rgba(30, 58, 95, 0.5);
    border-radius: 8px;
    gridline-color: rgba(30, 58, 95, 0.3);
    color: #e0e6ed;
    font-size: 13px;
    selection-background-color: rgba(0, 180, 216, 0.2);
    outline: none;
}
QTableWidget::item {
    padding: 12px 16px;
    border-bottom: 1px solid rgba(30, 58, 95, 0.25);
    min-height: 28px;
}
QTableWidget::item:selected {
    background-color: rgba(0, 180, 216, 0.15);
    color: #ffffff;
}
QTableWidget::item:hover:!selected {
    background-color: rgba(0, 180, 216, 0.08);
}
QHeaderView {
    background: transparent;
}
QHeaderView::section {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 rgba(20, 35, 55, 0.98), stop:1 rgba(15, 28, 45, 0.98));
    color: #00d4f8;
    font-weight: bold;
    font-size: 12px;
    padding: 12px 16px;
    border: none;
    border-bottom: 2px solid rgba(0, 180, 216, 0.5);
}
QHeaderView::section:first {
    border-top-left-radius: 8px;
}
QHeaderView::section:last {
    border-top-right-radius: 8px;
}

/* ===== 文本编辑框样式 ===== */
QTextEdit {
    background-color: rgba(13, 21, 32, 0.95);
    border: 1px solid rgba(30, 58, 95, 0.6);
    border-radius: 10px;
    color: #90caf9;
    font-family: "SF Mono", "Monaco", "Menlo", "Consolas", monospace;
    font-size: 11px;
    padding: 8px;
    selection-background-color: rgba(0, 180, 216, 0.3);
}

/* ===== 状态栏样式 ===== */
QStatusBar {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #0d1520, stop:1 #0a1218);
    color: #5a7a9a;
    border-top: 1px solid rgba(30, 58, 95, 0.5);
    padding: 4px 12px;
    font-size: 11px;
}

/* ===== 菜单栏样式 ===== */
QMenuBar {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #101824, stop:1 #0d1520);
    color: #b8c5d3;
    border-bottom: 1px solid rgba(30, 58, 95, 0.5);
    padding: 4px 8px;
    spacing: 4px;
}
QMenuBar::item {
    padding: 8px 16px;
    border-radius: 6px;
    font-weight: 500;
}
QMenuBar::item:selected {
    background: rgba(0, 180, 216, 0.2);
    color: #00b4d8;
}
QMenu {
    background-color: rgba(13, 21, 32, 0.98);
    color: #e0e6ed;
    border: 1px solid rgba(30, 58, 95, 0.8);
    border-radius: 10px;
    padding: 8px;
}
QMenu::item {
    padding: 10px 28px 10px 16px;
    border-radius: 6px;
    margin: 2px 4px;
}
QMenu::item:selected {
    background: rgba(0, 180, 216, 0.25);
    color: #00b4d8;
}
QMenu::separator {
    height: 1px;
    background: rgba(30, 58, 95, 0.6);
    margin: 6px 12px;
}

/* ===== 选项卡样式 ===== */
QTabWidget::pane {
    border: 1px solid rgba(30, 58, 95, 0.6);
    border-radius: 12px;
    background: rgba(13, 21, 32, 0.95);
    padding: 8px;
    top: -1px;
}
QTabBar::tab {
    background: rgba(13, 21, 32, 0.8);
    color: #6a8aa8;
    padding: 12px 24px;
    border: 1px solid rgba(30, 58, 95, 0.5);
    border-bottom: none;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    margin-right: 4px;
    font-weight: 500;
    font-size: 12px;
}
QTabBar::tab:selected {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #1a2a40, stop:1 #152535);
    color: #00b4d8;
    border-color: rgba(0, 180, 216, 0.5);
    border-bottom: 3px solid #00b4d8;
}
QTabBar::tab:hover:!selected {
    background: rgba(30, 58, 95, 0.4);
    color: #8aaac8;
}

/* ===== 滚动条样式 ===== */
QScrollBar:vertical {
    background: rgba(13, 21, 32, 0.5);
    width: 10px;
    border-radius: 5px;
    margin: 2px;
}
QScrollBar::handle:vertical {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                stop:0 #2e4a6f, stop:1 #1e3a5f);
    border-radius: 4px;
    min-height: 40px;
}
QScrollBar::handle:vertical:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                stop:0 #3e5a7f, stop:1 #2e4a6f);
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: transparent;
}
QScrollBar:horizontal {
    background: rgba(13, 21, 32, 0.5);
    height: 10px;
    border-radius: 5px;
    margin: 2px;
}
QScrollBar::handle:horizontal {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #2e4a6f, stop:1 #1e3a5f);
    border-radius: 4px;
    min-width: 40px;
}

/* ===== 工具提示样式 ===== */
QToolTip {
